{
  "$id": "https://barnevern.ssb.no/migration-report-request.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "migration-report-request",
  "description": "Migration report request",
  "required": [
    "municipalityId",
    "migrationYear"
  ],
  "properties": {
    "municipalityId": {
      "type": "string",
      "description": "Four-digit municipality ID, zero left padded",
      "pattern": "^[\\d]{4}$"
    },
    "migrationYear": {
      "type": "integer",
      "minimum": 2023,
      "description": "Year from municipality migration date"
    }
  }
}
